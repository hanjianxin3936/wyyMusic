/* pages/songList/songList.wxss */

.song-playmessage{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height:1000rpx;
    background-image: linear-gradient(to bottom left, rgba(32,34,33), rgba(121,126,129));
    z-index: 99;
    padding: 0 40rpx;
    padding-top: 80rpx;
    box-sizing: border-box;
}
.song-playmessage .song-playmessage-t{
    display: flex;
    flex-direction: row;
    height:200rpx;
    justify-content: space-between;
}
.cover-left{
    position: relative;
    width: 250rpx;
    height: 250rpx;
}
.cover-left image{
    width: 100%;
    border-radius: 10rpx;
}
.cover-left .playCount{
    position: absolute;
    top: 0;
    right: 0;
    margin-right: 10rpx;
    width: 100%;
    height: 30rpx;
    text-align: right;
    color: #fff;
    font-size: 22rpx;
}
.cover-right{
    display: flex;
    flex-wrap: wrap ;
    align-content: space-around;
    width: 360rpx;
    height: 260rpx;
}
.cover-right .title{
    color: #fff;
    font-size: 32rpx;
    font-weight: 700;
    line-height: 48rpx;
}
.cover-right .creator{
    color: #fff;
    font-size: 24rpx;
    padding-top: 20rpx;
}
.cover-right .description{
    padding-top: 20rpx;
    display: flex;
    color: #999;
    font-size: 22rpx;
    flex-direction: row;
    align-items: center;
}
.cover-right .iconfont.icon-jiantou{
    font-size: 22rpx;
}




.vip-audio{
    position:relative;
    width: 100%;
    height: 100rpx;
    background-color: #ccc;
    border-top-left-radius: 50rpx;
    border-top-right-radius: 50rpx;
    margin-top: 400rpx;
    z-index: 100;
}
.h{
    padding-top: 400rpx;
}
.song-wrap{
    position:relative;
    background-color: #ccc;
    z-index: 100;
} 
.song-cntent{
    width: 100%;
    border-top-left-radius: 50rpx;
    border-top-right-radius: 50rpx;
    background-color:#fff ;
    padding-left: 20rpx;
    box-sizing: border-box;
}
.vip-audio .song-item .icon-yinle{

   color: rgba(245, 64, 58);
   font-size: 46rpx;
   padding-left: 20rpx;
}
.song-top{
    height: 100rpx;
}
.song-top .song-prder.icon-bofang{
    font-size: 48rpx;
    color: #000;
}
.song-top-num{
    font-size: 24rpx;
    color: #999;
}
.song{
    display: flex;
    flex-direction: column;
}
.song-item{
    display: flex;
    flex-direction: row;
 
    height: 100rpx;
    align-items: center;
}
.song-item .song-prder{
  width: 80rpx;
  text-align: center;
  color: #999;
}

.song-item .song-audio-message .song-audio-title{
    width: 100%;
    font-size: 30rpx;
    line-height: 40rpx;
    overflow: hidden;
    /*文本不会换行*/
    white-space: nowrap;
    /*当文本溢出包含元素时，以省略号表示超出的文本*/
    text-overflow: ellipsis;
}
.song-item .song-audio-message{
    width: 470rpx;
}
.song-item .song-audio-message .song-audio-audo{
    width: 100%;
    font-size: 24rpx;
    overflow: hidden;
    /*文本不会换行*/
    white-space: nowrap;
    /*当文本溢出包含元素时，以省略号表示超出的文本*/
    text-overflow: ellipsis;
}

