/* pages/songLabel/songLabel.wxss */
.songLabel-item{
    height: auto;
    font-size: 30rpx;
    padding-top: 20rpx;
}
.songLabel-box{
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    align-content: space-around;
    padding: 30rpx 0rpx;
}
.songLabel-box .songLabel-box-item{
    flex: 0 0 25%;  
    text-align: center;
    max-height: 70rpx;
    line-height: 70rpx;
    margin-bottom: 20rpx;
    box-sizing: border-box;
    padding-right:20rpx;
    
}
.songLabel-box-item .text{
    white-space: nowrap;
    word-wrap: normal;
    border-radius: 40rpx;
    background-color: rgba(228, 228, 228, .5);
}
