/* components/nav/Nav.wxss */
.btn-nav{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0rpx;
}
.btn-nav navigator{
  display: block;
  text-align: center;
}
.iconfont{
  line-height: 90rpx;
}
.btn-nav navigator view{
  margin: 0 auto;
  background-color: #ff1d12;
  width: 90rpx;
  height: 90rpx;
  border-radius: 90rpx;
}
.btn-nav navigator text{
  display: block;
}
